oolwarm\nrcParams['axes.prop_cycle'] = cycler(color=cmap(np.linspace(0, 1, N)))\n\n\nfrom matplotlib.lines import Line2D\ncustom_lines = [Line2D([0], [0], color=cmap(0.), lw=4),\n                Line2D([0], [0], color=cmap(.5), lw=4),\n                Line2D([0], [0], color=cmap(1.), lw=4)]\n\nfig, ax = plt.subplots(figsize=(10, 5))\nlines = ax.plot(data)\nax.legend(custom_lines, ['Cold', 'Medium', 'Hot']);","key":"YCLnTd73pW"},{"type":"output","id":"Z29cbhk6HhsifT6acbCCU","data":[],"key":"szWR5ewyrn"}],"data":{"type":"notebook-code"},"key":"A2cmzUfsc3"},{"type":"block","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":2,"column":1}},"children":[{"type":"text","value":"There is a lot more that you can do with outputs (such as including interactive outputs)\nwith your book. For more information about this, see ","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"N2C7TFylmm"},{"type":"link","url":"https://jupyterbook.org","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"the Jupyter Book documentation","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Q00strBTD6"}],"urlSource":"https://jupyterbook.org","key":"pOPHG58hdK"}],"key":"acBZAg8tdf"}],"data":{"type":"notebook-content"},"key":"U5Dm7xpflL"}],"key":"bF8olUwEGZ"},"references":{"cite":{"order":[],"data":{}}}}